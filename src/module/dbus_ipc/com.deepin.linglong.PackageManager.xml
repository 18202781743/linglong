<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
        "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="com.deepin.linglong.PackageManager">
        <method name="Status">
            <arg name="Status" type="s" direction="out"/>
        </method>
        <method name="Download">
            <arg name="PackageIDList" type="as" direction="in"/>
            <arg name="savePath" type="s" direction="in"/>
            <arg name="RetMessageList" type="a(s)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="RetMessageList"/>
        </method>
        <method name="Install">
            <arg name="PackageIDList" type="as" direction="in"/>
            <arg name="paramMap" type="a{ss}" direction="in"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="ParamStringMap"/>
            <arg name="RetMessageList" type="a(s)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="RetMessageList"/>
        </method>
        <method name="Uninstall">
            <arg name="PackageIDList" type="as" direction="in"/>
            <arg name="paramMap" type="a{ss}" direction="in"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="ParamStringMap"/>
            <arg name="RetMessageList" type="a(s)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="RetMessageList"/>
        </method>
        <method name="Update">
            <arg name="Options" type="(aay)" direction="in"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.In0"
                        value="linglong::service::PackageManagerOptionList"/>
        </method>
        <method name="UpdateAll">
            <arg name="JobID" type="s" direction="out"/>
        </method>
        <method name="Query">
            <arg name="PackageIDList" type="as" direction="in"/>
            <arg name="paramMap" type="a{ss}" direction="in"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="ParamStringMap"/>
            <arg name="AppMetaInfoList" type="a(s)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="AppMetaInfoList"/>
        </method>
        <method name="Import">
            <arg name="PackageFileList" type="as" direction="in"/>
            <arg name="JobID" type="s" direction="out"/>
        </method>
        <method name="Start">
            <arg name="PackageId" type="s" direction="in"/>
            <arg name="paramMap" type="a{ss}" direction="in"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="ParamStringMap"/>
            <arg name="RetMessageList" type="a(bus)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="RetMessageList"/>
        </method>
        <method name="Stop">
            <arg name="ContainerID" type="s" direction="in"/>
            <arg name="RetMessageList" type="a(bus)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="RetMessageList"/>
        </method>
        <method name="ListContainer">
            <arg name="ContainerList" type="a(s)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="ContainerList"/>
        </method>
    </interface>
</node>