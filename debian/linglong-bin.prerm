#!/bin/sh

case "$1" in
remove | deconfigure | purge)
    killall ll-service >/dev/null 2<&1
    killall ll-system-helper >/dev/null 2<&1
    ;;
upgrade | failed-upgrade | abort-install | abort-upgrade | disappear) ;;

*)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

exit 0
